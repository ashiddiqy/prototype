#include <iostream>
using namespace std;

void masukkan_kalimat();
int tambah(int a, int b);

int main ()
{
	masukkan_kalimat();
	cout << " Penjumlahan (15,15) ="<<tambah(15,15)<<endl;
}
void masukkan_kalimat (){
	cout << " Praktikum Bahasa Pemograman Menggunakan C++"<<endl;
	cout << " Membuat function Prototype"<<endl;
}
int tambah (int a, int b)
{
	return a+b;
}
